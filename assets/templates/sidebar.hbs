<ul>
    {{#each this as |blog|}}
        <li>
            <a href="#{{ blog._id }}">
                <p class="title">{{ blog.title }}</p>
                <p class="post">{{ blog.post }}</p>
                <p class="date">{{ blog.createDate }}</p>
            </a>
        </li>
    {{/each}}
    <li class="newbutton">
        <a href="#new">
            <button>New</button>
        </a>
    </li>
</ul>
